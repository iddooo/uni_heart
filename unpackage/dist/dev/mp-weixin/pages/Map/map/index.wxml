<view class="page"><view class="map-container"><map style="width:100%;height:60vh;" scale="{{10}}" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{allMarkers}}" data-event-opts="{{[['markertap',[['checkStationInfo',['$event']]]]]}}" bindmarkertap="__e"></map></view><view class="resources-container"><view class="staion-info flex-ct"><view class="complex">{{''+station.complex+''}}</view><view data-event-opts="{{[['tap',[['preview',['$event']]]]]}}" class="preview" bindtap="__e">查看街景图片</view><image class="lead" src="/static/map/lead.png" mode data-event-opts="{{[['tap',[['lead',['$event']]]]]}}" bindtap="__e"></image></view><view class="describle flex-ct"><image src="/static/map/site-d.png" mode></image><view class="elips">{{"距您"+station.min_dis+" | "+station.address}}</view></view><view class="describle flex-ct"><image src="/static/map/time.png" mode></image><view>{{station.startDate+"至"+station.endDate+"  "+station.startTime+"-"+station.endTime}}</view></view><view class="describle flex-ct"><image src="/static/map/sno-img.png" mode></image><view>{{"编号："+station.sno}}</view></view><view class="dashed"></view><view hidden="{{!(type==1)}}"><resources vue-id="1" data="{{resources}}" bind:__l="__l"></resources></view><view hidden="{{!(type==2)}}"><kitchen-waste vue-id="2" data="{{station}}" bind:__l="__l"></kitchen-waste></view></view><block wx:if="{{visible}}"><view class="preview-pop"><banner vue-id="3" className="preview-banner" banners="{{pics}}" bind:__l="__l"></banner><view data-event-opts="{{[['tap',[['closePreview',['$event']]]]]}}" class="touch-close" bindtap="__e"><image src="/static/map/closepreview.png"></image></view></view></block></view>